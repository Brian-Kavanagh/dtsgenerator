{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2015",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": false,
        "sourceMap": true
    },
    "include": [
        "src/**/*.ts",
        "test/**/*.ts",
        "typings/bundle.d.ts"
    ],
    "exclude": [
        "test/expected_file/**/*.ts"
    ]
}
